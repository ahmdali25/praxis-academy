/HelloWorld.vue'\nimport Front from './components/Front.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    Front\n  },\n\n  data: () => {\n    return {\n      state: 'State',\n      show: true,\n      people: {\n        name: 'Ali',\n        age: 21\n      },\n      classes: ['Vue', 'Node', 'React Native'],\n      count: 0\n    }\n  },\n\n  beforeCreate() {\n    alert(this.life + \" before created\");\n  }\n\n  computed: {\n    getName() {\n      return this.people.name;\n    }\n  },\n\n  methods: {\n    changeName() {\n      this.people.name = 'Ahmad'\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n\n.btn{\n  margin: 10px;\n}\n</style>\n"]}]}